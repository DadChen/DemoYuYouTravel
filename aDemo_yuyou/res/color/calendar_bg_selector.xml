<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:state_selected="true" android:drawable="@drawable/calendar_item_sel_bg" />
    <item android:state_pressed="true" android:drawable="@drawable/today" />
    <item app:state_current_month="false">
        <color android:color="@color/white" />
    </item>
    <item android:drawable="@drawable/today" app:state_today="true" />
    <item>
        <color android:color="@color/white" />
    </item>
</selector>